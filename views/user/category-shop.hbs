<script>
	function addToWishlist(wishId) {
		
		$.ajax({
			url: '/add-wishlist/' + wishId,
			method: 'get',
			success: (response) => {
				if(response.login){
					Swal.fire({
						icon: 'info',
						title: 'Please Login',
						showConfirmButton: false,
					})
				}
				
				else if(response.exist){
					Swal.fire({
						icon: 'success',
						title: 'Already Added To Wishlist',
						showConfirmButton: true,
					})
				}else{
						Swal.fire({
						icon: 'success',
						title: 'Added To Wishlist',
						showConfirmButton: true,
					})
				}
				setTimeout(() => {

				}, 1200)
			}
		})
	}
</script>
<script>
	function addToCart(cartId) {
       
		$.ajax({
			url: '/add-cart/'+cartId,
			method: 'get',
			success: (response) => {
				if(response.login){
					Swal.fire({
						icon: 'info',
						title: 'Please Login',
						showConfirmButton:false,
					})
				}
				else if(response.exist){
					Swal.fire({
						icon: 'success',
						title: 'Already Added To Cart',
						showConfirmButton: true,
					})
				}
				else{
						Swal.fire({
						icon: 'success',
						title: 'Added To Cart',
						showConfirmButton: true,
					})
				}	
					setTimeout(() => {		
					}, 1200)
			}
		})
	}



</script>